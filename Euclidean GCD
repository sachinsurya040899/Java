import java.util.*;
public class Main {
    public static void main(String[] args) {
      Scanner sa=new Scanner(System.in);
      // To Find GCD Eculidean Formula
      //Formula GCD(a,b)=GCD(b,a mod b)
      int a=sa.nextInt();  //a=11
      int b=sa.nextInt();  //b=7
      int temp=0;
      while(b!=0) {
    	  temp=a;  	//temp=11
    	  a=b;		//a=7
    	  b=temp;	//b=11
    	  b=b%a;
      }
      System.out.print(a);
    }
}




Input:
12 4
Output:
4
